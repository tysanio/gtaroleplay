 	GetPlayerFacingAngle(playerid, ActorInfo[botsid][aFloat][3]);
	ActorInfo[botsid][actorint] = GetPlayerInterior(playerid);
  	ActorInfo[botsid][actorvw] = GetPlayerVirtualWorld(playerid);
	format(query, sizeof(query), "INSERT INTO `actors` (`Skinid`, `Float`, `actorint`, `actorvw`) VALUES ('%d', '%f|%f|%f|%f', '%d', '%d')", skin, ActorInfo[botsid][aFloat][0], ActorInfo[botsid][aFloat][1], ActorInfo[botsid][aFloat][2], ActorInfo[botsid][aFloat][3],ActorInfo[botsid][actorint], ActorInfo[botsid][actorvw]);
