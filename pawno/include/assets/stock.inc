Dialog:LotteryNumber(playerid, response, listitem, inputtext[])
{
	if (response)
	{
	    new bizid = Business_Inside(playerid);

	    if (bizid != -1)
	    {
	        if (isnull(inputtext) || (strval(inputtext) < 1 || strval(inputtext) > 60)) {
	            return Dialog_Show(playerid, LotteryNumber, DIALOG_STYLE_INPUT, "Numéro de lotterie", "S'il vous plaît entrez votre numéro de lotterie désiré ci-dessous (de 1-60):", "Envoyer", "Annuler");
			}
	        PlayerData[playerid][pLottery] = strval(inputtext);
	        PlayerData[playerid][pLotteryB] = 1;

		    GiveMoney(playerid, -BusinessData[bizid][bizPrices][0]);
			SendNearbyMessage(playerid, 30.0, COLOR_PURPLE, "** %s a payé %s $ et a reçu un billet de lotterie.", ReturnName(playerid, 0), FormatNumber(BusinessData[bizid][bizPrices][0]));

			BusinessData[bizid][bizProducts]--;
			BusinessData[bizid][bizVault] += Tax_Percent(BusinessData[bizid][bizPrices][6]);

			Business_Save(bizid);
			Tax_AddPercent(BusinessData[bizid][bizPrices][0]);
		}
	}
	return 1;
}

