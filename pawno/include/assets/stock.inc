	{
		foreach (new i : Player) {
		    SQL_SaveCharacter(i);
		    SetPlayerName(i,PlayerData[i][pUsername]);
			KickEx(i);
		}
		SendRconCommand("gmx");
	}
	else if (g_ServerRestart == 1) {
		GetElapsedTime(g_RestartTime--, time[0], time[1], time[2]);
		format(string, 32, "~r~Serveur Restart:~w~ %02d:%02d", time[1], time[2]);
