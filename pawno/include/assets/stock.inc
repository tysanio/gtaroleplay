Dialog:Inventory(playerid, response, listitem, inputtext[])
{
	if (response)
	{
	    new itemid = PlayerData[playerid][pInventoryItem],string[64];
	    strunpack(string, InventoryData[playerid][itemid][invItem]);
	    switch (listitem)
	    {
	        case 0:
	        {
	            if (!strcmp(string, "Demo Soda") && PlayerData[playerid][pTutorialStage] == 3)
			    {
        			PlayerData[playerid][pThirst] = 100;
					SetPlayerSpecialAction(playerid, SPECIAL_ACTION_NONE);
        			Dialog_Show(playerid, Tutorial, DIALOG_STYLE_MSGBOX, "Tutoriel", "Vous avez appris le fonctionnement du ramassage d'objet.\nVous venez de ramasser une cannette de soda afin de recharger votre soif..\n\nLes icônes de la faim et la soif sont affichés sur le côté droit de votre écran.\nSi votre soif ou la faim atteint les zéro pour cent, la vie de votre personnage diminuras.", "Continuer", "");
			    }
			    else
			    {
		            CallLocalFunction("OnPlayerUseItem", "dds", playerid, itemid, string);
				}
	        }
	        case 1:
	        {
	            if (!strcmp(string, "Demo Soda"))
	                return 0;

				PlayerData[playerid][pInventoryItem] = itemid;
				Dialog_Show(playerid, GiveItem, DIALOG_STYLE_INPUT, "Donner objet", "Entrée ID ou nom du joueur:", "Envoyer", "Annuler");
	        }
	        case 2:
	        {
	            new id = -1;

	            if (!strcmp(string, "Demo Soda") && PlayerData[playerid][pTutorialStage] != 4)
	                return 0;

				if (PlayerData[playerid][pTutorialStage] == 4)
				{
					Inventory_Remove(playerid, "Demo Soda");
					Dialog_Show(playerid, Tutorial, DIALOG_STYLE_MSGBOX, "Tutoriel", "Vous avez laissé tomber votre canette de soda. Utilisez la touche 'N' Pour ramassez un objet..\nVous pouvez aussi donner ou échanger des objets de votre inventaire à d'autres joueurs.\n\nVous pouvez également stocker des objets dans le coffre de votre maison ou le coffre d'un véhicule.\nL'inventaire de votre personnage ne peut contenir que %d éléments uniques à la fois.", "Next", "", MAX_INVENTORY);
					return 1;
				}
	            if (IsPlayerInAnyVehicle(playerid) || !IsPlayerSpawned(playerid))
	                return SendErrorMessage(playerid, "Vous ne pouvez pas déplacer des éléments à l'heure actuelle.");

				/*else if (!strcmp(string, "Backpack"))
					return cmd_dropbackpack(playerid, "\1");*/

				else if ((id = Garbage_Nearest(playerid)) != -1)
				{
				    if (GarbageData[id][garbageCapacity] >= 20)
				        return SendErrorMessage(playerid, "Cette poubelle est pleine de détritus.");

                    GarbageData[id][garbageCapacity]++;
                    Garbage_Save(id);

					Inventory_Remove(playerid, string);
                    SendNearbyMessage(playerid, 30.0, COLOR_PURPLE, "** %s jette un(e) \"%s\" dans une poubelle.", ReturnName(playerid, 0), string);

                    format(string, sizeof(string), "[Poubelle %d]\n{FFFFFF}Capacité: %d/20", id, GarbageData[id][garbageCapacity]);
                    UpdateDynamic3DTextLabelText(GarbageData[id][garbageText3D], COLOR_DARKBLUE, string);
				}
				else if (InventoryData[playerid][itemid][invQuantity] == 1)
					DropPlayerItem(playerid, itemid);

				else
					Dialog_Show(playerid, DropItem, DIALOG_STYLE_INPUT, "Jetter Objet", "Objet: %s - Quantité: %d\n\nCombien tu veux jeté:", "Jetter", "Annulé", string, InventoryData[playerid][itemid][invQuantity]);
	        }
	    }
	}
	return 1;
}

