				case 1:
				{
					new id = Inventory_GetItemID(playerid, string);

					if (!strcmp(string, "Backpack")) {
					    House_ShowItems(playerid, houseid);

						return SendErrorMessage(playerid, "Tu ne peut qu'avoir un sac a dos dans votre maison.");
					}
					else if (id == -1) {
						House_ShowItems(playerid, houseid);

