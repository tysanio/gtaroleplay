    static
		Float:x,
		Float:y,
		Float:z,
		Float:angle,
		id = -1;
	if ((id = PlayerData[playerid][pCarryCrate]) != -1 && CrateData[id][crateExists])
	{
		GetPlayerPos(playerid, x, y, z);
		GetPlayerFacingAngle(playerid, angle);
