Dialog:Mairie(playerid, response, listitem, inputtext[])
{
	if (response)
	{
		switch(listitem)
		{
			case 0: Dialog_Show(playerid, ChangeName, DIALOG_STYLE_INPUT, "Changer votre Nom en jeux", "Entrer votre nouveau nom IG", "Valider", "Annuler");
			case 1:
			{
				new amount = PlayerData[playerid][pBankMoney];
				SendServerMessage(playerid,"Voici votre paie au montant de %s$.",FormatNumber(amount));
				GiveMoney(playerid,amount);
				PlayerData[playerid][pBankMoney] = 0;
				return 1;
			}
		}
	}
	return 1;
}
