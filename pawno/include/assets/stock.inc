Dialog:ConfirmCarBuy(playerid, response, listitem, inputtext[])
{
	if (response)
	{
	    new bizid = Business_Inside(playerid),carid = PlayerData[playerid][pDealerCar],price = DealershipCars[bizid][carid][vehPrice];

		if (bizid != -1 && BusinessData[bizid][bizExists] && BusinessData[bizid][bizType] == 5)
		{
			if (PlayerData[playerid][pPlayingHours] < 15) {
				SendErrorMessage(playerid," Vous devez joueur 15 heures pour faire cela."); 
				SendErrorMessage(playerid, "Si vous trouver cela abusif fait /abus.");
				return 1;
			}
			if (GetMoney(playerid) < price)
			    return SendErrorMessage(playerid, "Vous n'avez pas assez.");

			if (Car_GetCount(playerid) >= MAX_OWNABLE_SOMETHING)
			    return SendErrorMessage(playerid, "Vous avez déjà %d vehicules (serveur limite).", MAX_OWNABLE_SOMETHING);

			new id = Car_Create(PlayerData[playerid][pID], DealershipCars[bizid][carid][vehModel], BusinessData[bizid][bizSpawn][0], BusinessData[bizid][bizSpawn][1], BusinessData[bizid][bizSpawn][2], BusinessData[bizid][bizSpawn][3], 1, 1,-1,-1);

			if (id != -1)
			{
			    Tax_AddPercent(price);

			    BusinessData[bizid][bizVault] += Tax_Percent(price);
			    Business_Save(bizid);

				SendServerMessage(playerid, "Vous avez acheter un/une %s pour %s!", ReturnVehicleModelName(DealershipCars[bizid][carid][vehModel]), FormatNumber(price));
				GiveMoney(playerid, -price);
				CarData[id][boitier] = 0;
				ShowPlayerFooter(playerid, "~w~ Vehicule ~p~ acheter!");
				Log_Write("logs/car_log.txt", "[%s] %s has purchased a %s pour %s.", ReturnDate(), ReturnName(playerid, 0), ReturnVehicleModelName(DealershipCars[bizid][carid][vehModel]), FormatNumber(price));
			}
		}
	}
	return 1;
}
