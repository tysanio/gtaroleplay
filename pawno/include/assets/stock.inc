LoadGarages()
{
	new Cache: result, ID, rows,countsuccess, countfailed;
	result = mysql_query(g_iHandle,"SELECT * FROM `garages` ORDER BY `ID` ASC", true);
	rows = cache_num_rows();
	if(rows)
	{
        for (new row; row < rows; row++)
        {
        	ID = cache_get_field_content_int(row, "ID");
            if ((ID < 1) || (ID >= MAX_GARAGES))
            {
                countfailed++;
                printf("*** ERROR: Invalid ID found in table \"garages\": %i", ID);
                continue;
            }
            cache_get_field_content(row, "Owner", GarageInfo[ID][Owner], .max_len = MAX_PLAYER_NAME);
            GarageInfo[ID][Owned] = cache_get_field_content_int(row, "Owned");
            GarageInfo[ID][Size] = cache_get_field_content_int(row, "Size");
            GarageInfo[ID][Price] = cache_get_field_content_int(row, "Price");
            GarageInfo[ID][eX] = cache_get_field_content_float(row,"eX");
            GarageInfo[ID][eY] = cache_get_field_content_float(row,"eY");
            GarageInfo[ID][eZ] = cache_get_field_content_float(row,"eZ");
            GarageInfo[ID][Lock] = cache_get_field_content_int(row, "portes");
            GarageInfo[ID][VirtualWorld] = ID;
            Garage_Update(ID);
            countsuccess++;
        }
	}
    printf("*** >>> Garages loaded: %i (successful: %i, failed: %i)", rows, countsuccess, countfailed);
    cache_delete(result);
}
Garage_Update(GarageID)
{
	new Msg[64],Float:x,Float:y,Float:z;
	x = GarageInfo[GarageID][eX];
	y = GarageInfo[GarageID][eY];
	z = GarageInfo[GarageID][eZ];
	if (IsValidDynamicPickup(GarageInfo[GarageID][PickupID])) DestroyDynamicPickup(GarageInfo[GarageID][PickupID]);
	if (IsValidDynamic3DTextLabel(GarageInfo[GarageID][Label])) DestroyDynamic3DTextLabel(GarageInfo[GarageID][Label]);
	if (GarageInfo[GarageID][Owned] == 1 && GarageInfo[GarageID][Lock] == 0)
	{
 		GarageInfo[GarageID][PickupID] = CreateDynamicPickup(1272, 1, x, y, z, 0);
		format(Msg, sizeof(Msg), "ID: %i\nProprio: %s\n Ouvert", GarageID, GarageInfo[GarageID][Owner]);
		GarageInfo[GarageID][Label] = CreateDynamic3DTextLabel(Msg, 0x008080FF, x, y, z + 1.0, 50.0);
	}
	else if (GarageInfo[GarageID][Owned] == 1 && GarageInfo[GarageID][Lock] == 1)
	{
 		GarageInfo[GarageID][PickupID] = CreateDynamicPickup(1272, 1, x, y, z, 0);
		format(Msg, sizeof(Msg), "ID: %i\nProprio: %s\n Fermer", GarageID, GarageInfo[GarageID][Owner]);
		GarageInfo[GarageID][Label] = CreateDynamic3DTextLabel(Msg, 0x008080FF, x, y, z + 1.0, 50.0);
	}
	else if (GarageInfo[GarageID][Owned] == 0)
	{
		GarageInfo[GarageID][PickupID] = CreateDynamicPickup(1273, 1, x, y, z, 0);
		format(Msg, sizeof(Msg), "Garage valide pour\n%i$\n/achetergarage", GarageInfo[GarageID][Price]);
		GarageInfo[GarageID][Label] = CreateDynamic3DTextLabel(Msg, 0x008080FF, x, y, z + 1.0, 50.0);
	}
}
Garage_SetOwner(playerid, GarageID)
{
	new Name[MAX_PLAYER_NAME], Msg[128],moneyentrepriseid;
	GetPlayerName(playerid, Name, sizeof(Name));
	GarageInfo[GarageID][Owned] = 1;
	GarageInfo[GarageID][Lock] = 1;
	strcat(GarageInfo[GarageID][Owner], Name, sizeof(Name));
	GiveMoney(playerid,-GarageInfo[GarageID][Price]);
	argent_entreprise[moneyentrepriseid][argentmairie] += GarageInfo[GarageID][Price];
	moneyentreprisesave(moneyentrepriseid);
	Garage_Update(GarageID);
	mysql_format(g_iHandle, Msg, sizeof(Msg), "UPDATE `garages` SET `Owner` = '%e' , `Owned` = 1 WHERE `ID` = %i" , GarageInfo[GarageID][Owner] , GarageID);
	mysql_tquery(g_iHandle, Msg);
	SendServerMessage(playerid,"Vous avez acheter se garage pour $%i", GarageInfo[GarageID][Price]);
	return 1;
}
