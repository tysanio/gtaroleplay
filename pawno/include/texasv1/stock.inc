	                return SendErrorMessage(playerid, "Vous ne pouvez pas déplacer des éléments à l'heure actuelle.");

				/*else if (!strcmp(string, "Backpack"))
					return cmd_dropbackpack(playerid, "\1");*/

				else if ((id = Garbage_Nearest(playerid)) != -1)
				{
				    if (GarbageData[id][garbageCapacity] >= 20)
				        return SendErrorMessage(playerid, "Cette poubelle est pleine de détritus.");

                    GarbageData[id][garbageCapacity]++;
                    Garbage_Save(id);

					Inventory_Remove(playerid, string);
                    SendNearbyMessage(playerid, 30.0, COLOR_PURPLE, "** %s jette un(e) \"%s\" dans une poubelle.", ReturnName(playerid, 0), string);

                    format(string, sizeof(string), "[Poubelle %d]\n{FFFFFF}Capacité: %d/20", id, GarbageData[id][garbageCapacity]);
                    UpdateDynamic3DTextLabelText(GarbageData[id][garbageText3D], COLOR_DARKBLUE, string);
				}
				else if (InventoryData[playerid][itemid][invQuantity] == 1)
					DropPlayerItem(playerid, itemid);

				else
					Dialog_Show(playerid, DropItem, DIALOG_STYLE_INPUT, "Jetter Objet", "Objet: %s - Quantité: %d\n\nCombien tu veux jeté:", "Jetter", "Annulé", string, InventoryData[playerid][itemid][invQuantity]);
	        }
	    }
	}
	return 1;
}

