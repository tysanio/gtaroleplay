stock GetPointInAngleOfPlayer(playerid, &Float:x, &Float:y, &Float:z, Float:distance, Float:angle){
	new Float:current_angle;
	GetPlayerPos(playerid, x, y, z);
	GetPlayerFacingAngle(playerid, current_angle);
	if(IsPlayerInAnyVehicle(playerid))
	{
		GetVehicleZAngle(GetPlayerVehicleID(playerid), current_angle);
 	}
	new Float:a = current_angle + angle;
	x += (distance * floatsin(-a, degrees));
	y += (distance * floatcos(-a, degrees));
}
//decoder
Dialog:decodeur(playerid, response, listitem, inputtext[])
{
	if(!response) return 0;
	if (response)
	{
		switch(listitem)
		{
			case 0:
			{
				new rand = random(100),id = Gate_Nearest(playerid);
				if (id != -1)
				{
					if (strlen(GateData[id][gatePass]))
					{
						switch(rand)
						{				
							case 0:
							{	
								SendServerMessage(playerid,"Voici le code de la porte %s",GateData[id][gatePass]);
								return 1;
							}
							case 1 .. 99:
							{
								new bizid = Entrance_Inside(playerid),factionid = PlayerData[playerid][pFaction];
								if(PlayerData[playerid][pHideTags] > 0) return SendErrorMessage(playerid,"Vous devez encore attendre quelque temps avant de refaire cela.");
								if(FactionData[factionid][factionacces][1] == 1)
								{ SendServerMessage(playerid, "RADIO: Quelqu'un a essayer de forcé le code de la porte a %s (marquée sur la carte).",EntranceData[bizid][entranceName]);}
								foreach (new i : Player)
								{
									if (IsACops(i)) {Waypoint_Set(i, "Tentative de forcage de porte",EntranceData[bizid][entrancePos][0], EntranceData[bizid][entrancePos][1], EntranceData[bizid][entrancePos][2]);}
								}
								PlayerData[playerid][pHideTags] = 3;
								return 1;
							}
						}
					}
					else SendServerMessage(playerid,"Cette porte n'a pas de mot de passe.");
					return 1;
				}
				else SendErrorMessage(playerid,"Vous n'etes pas proche d'une porte a décoder.");
				return 1;
			}
			case 1:
			{
				new stockjobinfoid,moneyentrepriseid,money = random(500) + 100,Float:x,Float:y,Float:z,facass = PlayerData[playerid][pFaction];
				if (ATM_Nearest(playerid) == -1)
					return SendErrorMessage(playerid, "Vous n'êtes pas proche d'un atm.");
				if (!Inventory_HasItem(playerid, "Decodeur"))
					return SendErrorMessage(playerid, "Vous n'avez pas de decodeur.");
				if(PlayerData[playerid][pHideTags] > 0) return SendErrorMessage(playerid,"Vous devez encore attendre quelque temps avant de refaire cela.");
				if(info_stockjobinfo[stockjobinfoid][stockjobinfocentral1] == 0 && info_stockjobinfo[stockjobinfoid][stockjobinfocentral2] == 0 && info_stockjobinfo[stockjobinfoid][stockjobinfocentral3] == 0 && info_stockjobinfo[stockjobinfoid][stockjobinfocentral4] == 0 && info_stockjobinfo[stockjobinfoid][stockjobinfocentral5] == 0) return SendErrorMessage(playerid,"Il n'y a plus d'électricité dans le pays central générateur hors service");
				new id = Inventory_Add(playerid, "argent sale", 1212,money);
				if (id == -1)
					return SendErrorMessage(playerid, "Inventaire plein.");
				GetPlayerPos(playerid,x,y,z);
				argent_entreprise[moneyentrepriseid][argentbanque] -= money;
				moneyentreprisesave(moneyentrepriseid);
				if(FactionData[facass][factionacces][1] == 1)
				{ SendServerMessage(playerid, "RADIO: Un vole d'atm a été signalé vers %s.", GetLocation(x,y,z));}
				SendServerMessage(playerid,"Vous aver voler un atm et vous avez prit %d$",money);
				//generateur
				if( info_stockjobinfo[stockjobinfoid][stockjobinfocentral1] > 0) {info_stockjobinfo[stockjobinfoid][stockjobinfocentral1] -= 10;}
				if( info_stockjobinfo[stockjobinfoid][stockjobinfocentral2] > 0) {info_stockjobinfo[stockjobinfoid][stockjobinfocentral2] -= 10;}
				if( info_stockjobinfo[stockjobinfoid][stockjobinfocentral3] > 0) {info_stockjobinfo[stockjobinfoid][stockjobinfocentral3] -= 10;}
				if( info_stockjobinfo[stockjobinfoid][stockjobinfocentral4] > 0) {info_stockjobinfo[stockjobinfoid][stockjobinfocentral4] -= 10;}
				if( info_stockjobinfo[stockjobinfoid][stockjobinfocentral5] > 0) {info_stockjobinfo[stockjobinfoid][stockjobinfocentral5] -= 10;}
				if( info_stockjobinfo[stockjobinfoid][stockjobinfocentral1] < 0) {info_stockjobinfo[stockjobinfoid][stockjobinfocentral1] = 0;}
				if( info_stockjobinfo[stockjobinfoid][stockjobinfocentral2] < 0) {info_stockjobinfo[stockjobinfoid][stockjobinfocentral2] = 0;}
				if( info_stockjobinfo[stockjobinfoid][stockjobinfocentral3] < 0) {info_stockjobinfo[stockjobinfoid][stockjobinfocentral3] = 0;}
				if( info_stockjobinfo[stockjobinfoid][stockjobinfocentral4] < 0) {info_stockjobinfo[stockjobinfoid][stockjobinfocentral4] = 0;}
				if( info_stockjobinfo[stockjobinfoid][stockjobinfocentral5] < 0) {info_stockjobinfo[stockjobinfoid][stockjobinfocentral5] = 0;}
				stockjobinfosave(stockjobinfoid);
				PlayerData[playerid][pHideTags] = 3;
				return 1;
			}			
		}
	}
	return 1;
}
