Dialog:LockerMoneyRetirer(playerid,response,listitem,inputtext[])
{
	new factionid = PlayerData[playerid][pFaction],amount = strval(inputtext);
	if (factionid == -1 || !IsNearFactionLocker(playerid))
		return 0;
	if (response)
	{
		if (amount < 1 || amount > FactionData[factionid][factioncoffre]) {SendErrorMessage(playerid,"Vous n'avez pas asser d'argent dans le coffre vous avez %d",FactionData[factionid][factioncoffre]);}
		SendServerMessage(playerid,"Vous avez retirer %d du coffre faction.",amount);
		FactionData[factionid][factioncoffre] -= amount;
		GiveMoney(playerid, amount);
		Faction_Save(factionid);
		Log_Write("logs/faction_coffre.txt", "%s %s a prie %d", ReturnDate(), ReturnName(playerid, 0),amount);
		Dialog_Show(playerid, LockerMoney, DIALOG_STYLE_LIST, "Coffre d'argent","Argent dans le coffre %d\nRetirer du coffre\nMettre dans le coffre", "Choisir", "Annuler",FactionData[factionid][factioncoffre]);
	}
	return 1;
}
Dialog:LockerMoneyAjouter(playerid,response,listitem,inputtext[])
{
	new factionid = PlayerData[playerid][pFaction],amount = strval(inputtext);
	if (factionid == -1 || !IsNearFactionLocker(playerid))
		return 0;
	if (response)
	{
		if (amount > GetMoney(playerid)) return SendErrorMessage(playerid, "Vous n'avez pas assez sur vous..");
		SendServerMessage(playerid,"Vous avez mis %d du coffre faction.",amount);
		FactionData[factionid][factioncoffre] += amount;
		GiveMoney(playerid, -amount);
		Faction_Save(factionid);
		Log_Write("logs/faction_coffre.txt", "%s %s a ajouter %d", ReturnDate(), ReturnName(playerid, 0),amount);
		Dialog_Show(playerid, LockerMoney, DIALOG_STYLE_LIST, "Coffre d'argent","Argent dans le coffre %d\nRetirer du coffre\nMettre dans le coffre", "Choisir", "Annuler",FactionData[factionid][factioncoffre]);
	}
	return 1;
}
Dialog:LockerWeapons(playerid, response, listitem, inputtext[])
{
	new factionid = PlayerData[playerid][pFaction];
	if (factionid == -1 || !IsNearFactionLocker(playerid))
		return 0;
	if (response)
	{
	    new weaponid = FactionData[factionid][factionWeapons][listitem],ammo = FactionData[factionid][factionAmmo][listitem];

	    if (weaponid)
		{
	        if (PlayerHasWeapon(playerid, weaponid))
	            return SendErrorMessage(playerid, "Vous avez déjà cette arme.");

	        GiveWeaponToPlayer(playerid, weaponid, ammo);
	        SendNearbyMessage(playerid, 30.0, COLOR_PURPLE, "** %s cherche dans le casier et s'équipe à l'aide de sa main d'un(e) %s.", ReturnName(playerid, 0), ReturnWeaponName(weaponid));
			if (FactionData[factionid][factionacces][8] == 1|| FactionData[factionid][factionacces][12] == 1|| FactionData[factionid][factionacces][6] == 1 || FactionData[factionid][factionacces][13] == 1 || FactionData[factionid][factionacces][9] == 1)
		    {
		        FactionData[factionid][factionWeapons][listitem] = 0;
		        FactionData[factionid][factionAmmo][listitem] = 0;
		        Faction_Save(factionid);
			}
		}
		else
		{
		    if (FactionData[factionid][factionacces][8] == 1|| FactionData[factionid][factionacces][12] == 1|| FactionData[factionid][factionacces][6] == 1 || FactionData[factionid][factionacces][13] == 1 || FactionData[factionid][factionacces][9] == 1)
		    {
		        if ((weaponid = GetWeapon(playerid)) == 0)
		            return SendErrorMessage(playerid, "Vous n'avez pas d'arme sur toi.");
		        FactionData[factionid][factionWeapons][listitem] = weaponid;
		        FactionData[factionid][factionAmmo][listitem] = GetPlayerAmmo(playerid);
		        Faction_Save(factionid);
                ResetWeapon(playerid, weaponid);
		        SendNearbyMessage(playerid, 30.0, COLOR_PURPLE, "** %s sort un(e) %s et le range dans le casier.", ReturnName(playerid, 0), ReturnWeaponName(weaponid));
			}
			else
			{
			    SendErrorMessage(playerid, "Le slots désiré est vide.");
			}
	    }
	}
	else {
	    cmd_fequipement(playerid, "\1");
	}
	return 1;
}

