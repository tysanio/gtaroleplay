stock DeleteCandidate(candidate) // ???????? ?????????
{
	new query[900];
    format(query,sizeof(query), "DELETE FROM `candidates` WHERE `Name` = '%s'", Candidates1[candidate]);
    mysql_tquery(g_iHandle, query);
    format(Candidates1[candidate], MAX_PLAYER_NAME, Candidates1[--NumberOfCandidates]);
    Votes[candidate] = Votes[NumberOfCandidates];
	return 1;
}
stock SaveCandidate(candidate) // ?????????? ?????????
{
	new query[900];
    format(query,sizeof(query), "UPDATE `candidates` SET `Votes` = '%d' WHERE `Name` = '%s'", Votes[candidate], Candidates1[candidate]);
    mysql_tquery(g_iHandle, query);
	return 1;
}
//limiteur
stock SetVehicleSpeed(vehicleid,mph)
{
	new Float:Vx,Float:Vy,Float:Vz,Float:DV,Float:multiple;
	GetVehicleVelocity(vehicleid,Vx,Vy,Vz);
	DV = floatsqroot(Vx*Vx + Vy*Vy + Vz*Vz);
	if(DV > 0)
	{
		multiple = (mph / (DV * 180));
		return SetVehicleVelocity(vehicleid,Vx*multiple,Vy*multiple,Vz*multiple);
	}
	return 0;
}
