//caisse
stock caisse_Refresh(id)
{
	if (id != -1 && caisseMachineData[id][caisseExists])
	{
		if (IsValidDynamicObject(caisseMachineData[id][caisseObject]))
			DestroyDynamicObject(caisseMachineData[id][caisseObject]);
		caisseMachineData[id][caisseObject] = CreateDynamicObject(1514, caisseMachineData[id][caissePos][0], caisseMachineData[id][caissePos][1], caisseMachineData[id][caissePos][2], caisseMachineData[id][caissePos][3], caisseMachineData[id][caissePos][4], caisseMachineData[id][caissePos][5], caisseMachineData[id][caisseWorld], caisseMachineData[id][caisseInterior]);
	}
	return 1;
}
stock caisse_Create(playerid)
{
	new
	    Float:x,
	    Float:y,
	    Float:z,
	    Float:angle;
	if (GetPlayerPos(playerid, x, y, z) && GetPlayerFacingAngle(playerid, angle))
	{
		for (new i = 0; i < MAX_caisseS; i ++) if (!caisseMachineData[i][caisseExists])
		{
		    caisseMachineData[i][caisseExists] = true;
			caisseMachineData[i][caissePos][0] = x + (1.0 * floatsin(-angle, degrees));
			caisseMachineData[i][caissePos][1] = y + (1.0 * floatcos(-angle, degrees));
			caisseMachineData[i][caissePos][2] = z;
			caisseMachineData[i][caissePos][3] = 0.0;
			caisseMachineData[i][caissePos][4] = 0.0;
			caisseMachineData[i][caissePos][5] = angle;
            caisseMachineData[i][caisseInterior] = GetPlayerInterior(playerid);
            caisseMachineData[i][caisseWorld] = GetPlayerVirtualWorld(playerid);
			caisse_Refresh(i);
			mysql_tquery(g_iHandle, "INSERT INTO `caisses` (`caisseWorld`) VALUES(0)", "OncaisseCreated", "d", i);
			return i;
		}
	}
	return -1;
}
stock caisse_Delete(caisseid)
{
	if (caisseid != -1 && caisseMachineData[caisseid][caisseExists])
	{
		new query[64];
		format(query, sizeof(query), "DELETE FROM `caisses` WHERE `caisseID` = '%d'", caisseMachineData[caisseid][caisseID]);
		mysql_tquery(g_iHandle, query);
		if (IsValidDynamicObject(caisseMachineData[caisseid][caisseObject]))
		    DestroyDynamicObject(caisseMachineData[caisseid][caisseObject]);
		for (new i = 0; i != MAX_caisseS; i ++) if (caisseMachineData[i][caisseExists]) {
		    caisse_Save(i);
		}
	    caisseMachineData[caisseid][caisseExists] = false;
	    caisseMachineData[caisseid][caisseID] = 0;
	}
	return 1;
}
stock caisse_Save(caisseid)
{
	new query[300];
	format(query, sizeof(query), "UPDATE `caisses` SET `caisseX` = '%.4f', `caisseY` = '%.4f', `caisseZ` = '%.4f', `caisseRX` = '%.4f', `caisseRY` = '%.4f', `caisseRZ` = '%.4f', `caisseInterior` = '%d', `caisseWorld` = '%d' WHERE `caisseID` = '%d'",
	    caisseMachineData[caisseid][caissePos][0],
	    caisseMachineData[caisseid][caissePos][1],
	    caisseMachineData[caisseid][caissePos][2],
	    caisseMachineData[caisseid][caissePos][3],
	    caisseMachineData[caisseid][caissePos][4],
	    caisseMachineData[caisseid][caissePos][5],
	    caisseMachineData[caisseid][caisseInterior],
	    caisseMachineData[caisseid][caisseWorld],
	    caisseMachineData[caisseid][caisseID]
	);
	return mysql_tquery(g_iHandle, query);
}
stock caisse_Nearest(playerid)
{
	for (new i = 0; i < MAX_caisseS; i ++) if (caisseMachineData[i][caisseExists] && IsPlayerInRangeOfPoint(playerid, 1.0, caisseMachineData[i][caissePos][0], caisseMachineData[i][caissePos][1], caisseMachineData[i][caissePos][2]))
	    return i;
	return -1;
}
