				Dialog_Show(playerid, GiveItem, DIALOG_STYLE_INPUT, "Donner objet", "Entrée ID ou nom du joueur:", "Envoyer", "Annuler");
	        }
	        case 2:
	        {
	            new id = -1;

	            if (!strcmp(string, "Demo Soda") && PlayerData[playerid][pTutorialStage] != 4)
	                return 0;

